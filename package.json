{
  "name": "nunjucks-tags-typography",
  "version": "0.0.0",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run _test && nyc report --reporter=html",
    "test:ci": "npm run _test && nyc report --reporter=lcov > coverage.lcov && codecov",
    "lint:fix": "standard --fix",
    "lint:code": "codeclimate analyze",
    "_test": "standard && nyc --all mocha"
  },
  "author": "Jon Lauridsen <mail@jonlauridsen.com>",
  "license": "ISC",
  "dependencies": {
    "titlecase": "^1.1.2"
  },
  "peerDependencies": {
    "nunjucks-tags": "git+https://github.com/gaggle/nunjucks-tags.git"
  },
  "devDependencies": {
    "codecov": "^2.3.0",
    "dom-compare": "^0.3.1",
    "mocha": "^3.5.0",
    "mock-fs": "^4.4.1",
    "nunjucks-tags": "git+https://github.com/gaggle/nunjucks-tags.git",
    "nyc": "^11.1.0",
    "recursive-readdir-sync": "^1.0.6",
    "standard": "^10.0.3",
    "xmldom": "^0.1.27"
  },
  "nyc": {
    "all": true,
    "include": [
      "lib/**/*.js",
      "test/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js"
    ],
    "reporter": [
      "text-summary",
      "html"
    ]
  }
}
