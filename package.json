{
  "name": "nunjucks-tags-typography",
  "version": "0.0.0",
  "main": "lib/index.js",
  "scripts": {
    "lint:code": "codeclimate analyze",
    "lint:fix": "standard --fix",
    "test": "npm run test:mocha && standard",
    "test:ci": "npm run test:mocha && nyc report --reporter=lcov > coverage.lcov && codecov",
    "test:mocha": "nyc --all mocha && nyc report --reporter=html"
  },
  "author": "Jon Lauridsen <mail@jonlauridsen.com>",
  "license": "ISC",
  "dependencies": {
    "domurl": "^2.1.7",
    "highlight.js": "^10.4.1",
    "html-entities": "^1.2.1",
    "lodash.merge": "^4.6.0",
    "strip-indent": "^2.0.0",
    "titlecase": "^1.1.2"
  },
  "devDependencies": {
    "codecov": "^2.3.0",
    "dom-compare": "^0.3.1",
    "eslint": "^4.10.0",
    "lodash.isstring": "^4.0.1",
    "mocha": "^3.5.0",
    "mock-fs": "^4.4.1",
    "nunjucks-tags": "github:gaggle/nunjucks-tags",
    "nyc": "^11.1.0",
    "recursive-readdir-sync": "^1.0.6",
    "rewire": "^2.5.2",
    "standard": "^10.0.3",
    "xmlcompare": "github:gaggle/xmlcompare",
    "xmldom": "^0.1.27"
  },
  "nyc": {
    "all": true,
    "include": [
      "lib/**/*.js",
      "test/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js"
    ],
    "reporter": [
      "text-summary",
      "html"
    ]
  }
}
